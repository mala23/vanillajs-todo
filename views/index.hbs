 <h1>VanillaJS To Do</h1>
   <form action="/" method="post">
     <input id="todoText" type="text" name="todoText" value="">
     <button id="todo-add" type="submit" name="todo-button">+</button>
   </form>

 <section>
   <ul id="todo-list">
     {{#each todos}}
       <li>
         <p>{{{ this.text }}}</p>
       </li>
     {{/each}} 
   </ul>
 </section>
